{
	"name": "multiple-files-renamer",
	"displayName": "Multiple Files Renamer",
	"description": "Renames multiple files at once by replacing a substring of each filename using regex or matching a substring.",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.32.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.renameFiles"
	],
	"main": "./out/extension.js",
	"contributes": {
		"menus": {
				"explorer/context": [
						{
								"command": "extension.renameFiles",
								"group": "1_modification",
								"when": "explorerViewletVisible"
						}
				]
		},
		"commands": [
			{
				"command": "extension.renameFiles",
				"title": "Rename Files"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"tslint": "^5.12.1",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	}
}
